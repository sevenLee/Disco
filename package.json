  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "start:localdev": "NODE_ENV=localdev node server.js",
    "build:dev": "webpack --config ./config/webpack.config.dev.js --progress",
    "build:staging": "webpack --config ./config/webpack.config.staging.js --progress",
    "build:prod": "webpack --config ./config/webpack.config.prod.js --progress",
    "ci:development": "npm run clean && npm run build:dev && npm run start",
    "ci:staging": "npm run clean && npm run build:staging && npm run start",
    "ci:production": "npm run clean && npm run build:prod && npm run start",
    "bslocal:dev": "npm run clean && npm run build:dev && cross-env NODE_ENV=development node server.js",
    "bslocal:staging": "npm run clean && npm run build:staging && cross-env NODE_ENV=staging node server.js",
    "bslocal:prod": "npm run clean && npm run build:prod && cross-env NODE_ENV=production node server.js",
    "clean": "rimraf dist",
    "lint": "eslint -c .eslintrc -f checkstyle app/**/* config/* i18n/* store/* > lintresults.xml || echo 'Linting failed...' ",
    "version": "echo $npm_package_version"
  },
